pipeline {
    agent any

    stages {
        stage('Run A Command') {
            steps {
                sh 'date'
            }
        }
         stage('Test') {
            steps {
                echo 'Hello test'
            }
        }
         stage('Build') {
            steps {
                echo 'Hello Build'
            }
        }
         stage('Test to Deploy') {
            steps {
                echo 'Hello Deploy'
            }
        }
         stage('Test To Prod') {
            steps {
                echo 'Hello Prod'
            }
        }
    }
}
